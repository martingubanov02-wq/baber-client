<%- include('partials/layout', { title: 'Регистрация' }) %>
<section class="card small">
  <h2>Регистрация</h2>
  <form method="post" action="/register" autocomplete="off">
    <label>Ник</label>
    <input type="text" name="username" required minlength="3" maxlength="32" autocomplete="off" autocapitalize="off" autocorrect="off" spellcheck="false" />
    <label>Ключ</label>
    <input type="password" name="key" required minlength="6" maxlength="128" placeholder="Вставьте выданный ключ" autocomplete="new-password" autocapitalize="off" autocorrect="off" spellcheck="false" />
    <button class="btn" type="submit">Создать аккаунт</button>
  </form>
</section>
<%- include('partials/footer') %>
